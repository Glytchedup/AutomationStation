    "babel-preset-latest": "^6.24.1",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.15.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.0.17",
    "node-cmd": "^3.0.0",
    "react-switch": "^3.0.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
